[autoload]

Main="*res://GMTK_Jam/script/Main.gd"
SoundManager="*res://GMTK_Jam/Nodes/AutoLoad/SoundManager.tscn"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
